{
  "name": "ios-calculator",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "6.5.0"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "decimal.js": "^6.0.0",
    "http-server": "^0.9.0",
    "jake": "^8.0.12",
    "jshint": "^2.9.3",
    "karma": "^1.2.0",
    "karma-chai": "^0.1.0",
    "karma-commonjs": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-sinon": "^1.0.5",
    "mocha": "^3.0.2",
    "semver": "^5.3.0",
    "shelljs": "^0.7.4",
    "simplebuild-jshint": "^1.3.0",
    "simplebuild-karma": "^1.0.0",
    "sinon": "^1.17.5"
